<Window x:Class="VKMusicPlayer.TwoFactorAuth"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VKMusicPlayer"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="TwoFactorAuth" SizeToContent="WidthAndHeight" ResizeMode="CanMinimize">
    <Grid Height="100" Width="250">
        <Label Content="Введите код двухэтапной аутентификации" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TwoFactorCodeTextBox" HorizontalAlignment="Left" Margin="5,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="235"/>
        <Button Content="Далее" HorizontalAlignment="Center" Margin="0,64,0,0" VerticalAlignment="Top" IsDefault="True" Click="Button_Click"/>

    </Grid>
</Window>
