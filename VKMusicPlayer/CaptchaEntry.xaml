<Window x:Class="VKMusicPlayer.CaptchaEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VKMusicPlayer"
        mc:Ignorable="d"
        Title="CaptchaEntry" SizeToContent="WidthAndHeight">
    <Grid Height="175" Width="200">
        <Image x:Name="MainImage" HorizontalAlignment="Left" Height="100" Margin="0,10,0,0" VerticalAlignment="Top" Width="196"/>
        <TextBox x:Name="TextBox" HorizontalAlignment="Center" Margin="0,122,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Далее" HorizontalAlignment="Center" Margin="0,151,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
